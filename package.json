{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "deezer",
  "title": "Deezer",
  "description": "Control Deezer in browser",
  "icon": "command-icon.png",
  "author": "7rulnik",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Update Bookmarks",
      "subtitle": "fsdfsdf",
      "description": "Template for showing a menu bar extra",
      "mode": "menu-bar",
      "interval": "5s"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}